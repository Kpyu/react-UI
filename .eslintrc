{
 "parser": "babel-eslint",
 "globals": {
    "define": true,                           //requirejs
    "require": true,                          //requirejs
    "previewOutCall": true,
    "setLocalStorageLogin": true,
    // lambda表达式  
    "arrowFunctions": true,  
    // 解构赋值  
    "destructuring": true,
    "restParams": true,  
    "experimentalObjectRestSpread": true  
  },
  "extends": "airbnb",
  "rules": {
    "comma-dangle": [2, "never"],
    "prefer-const": 0,
    "no-var": 0,
    "prefer-arrow-callback": 0,
    "func-names": 0,
    "space-before-function-paren": [2, "never"],
    "prefer-template": 0,
    "no-console": 0,
    "object-shorthand": 0,
    "no-param-reassign": 0,
    "no-else-return": 0,
    "arrow-body-style":0
  }
}